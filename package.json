{
  "name": "@rsksmart/rif-scheduler-sdk",
  "version": "1.0.0",
  "description": "RIF Scheduler SDK",
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:ci": "npx jest src/*/* --coverage --ci",
    "lint": "npx eslint . src/ test/ --ext .js,.ts",
    "lint:fix": "npx eslint . src/ test/ --ext .js,.ts --fix",
    "build": "npx webpack",
    "postinstall": "typechain \"src/contracts/*.json\" --target=ethers-v5 --out-dir=src/contracts/types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsksmart/rif-scheduler-sdk.git"
  },
  "keywords": [
